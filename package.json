{
  "name": "bill-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "antd-mobile": "^2.3.4",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "husky": "^4.3.7",
    "js-cookie": "^2.2.1",
    "lib-flexible": "^0.3.2",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stylelint": "^13.8.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{jsx}": "npm run lint-staged:js",
    "**/*.{ts,tsx}": "npm run lint-staged:js"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-app-rewired start",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style --ignore-path .eslintignore .",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src --ignore-path .eslintignore .",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "check-prettier lint --ignore-path .eslintignore .",
    "prettier": "prettier -c --write \"**/*\"",
    "make:dir": "mddir ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/js-cookie": "^2.2.6",
    "@types/mockjs": "^1.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@umijs/fabric": "^2.5.2",
    "babel-plugin-import": "^1.13.3",
    "classname": "^0.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "customize-cra": "^1.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "less": "^4.1.0",
    "less-loader": "^5.0.0",
    "mddir": "^1.1.1",
    "mockjs": "^1.1.0",
    "postcss-pxtorem": "^5.1.1",
    "react-app-rewired": "^2.1.8",
    "stylelint": "^13.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
